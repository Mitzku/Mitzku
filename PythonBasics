def mult_two_add_three(number):
  print(number*2 + 3)
  
# Call mult_two_add_three() here:
mult_two_add_three(0)


def mult_x_add_y(number, x, y):
  print(number*x + y)
  
mult_x_add_y(1, 3, 1)
