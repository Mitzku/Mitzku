# We've defined the variable "meal" here to the name of the food we ate for breakfast!
meal = "An english muffin"

# Printing out breakfast
print("Breakfast:")
print(meal)

# Now update meal to be lunch!
meal = "rice"

# Printing out lunch
print("Lunch:")
print(meal)

# Now update "meal" to be dinner
meal = "salad"

# Printing out dinner
print("Dinner:")
print(meal)


